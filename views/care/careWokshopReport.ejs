<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Care Workshop Report</title>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.7.2/css/all.min.css" />
    <link rel="stylesheet" href="/bootstrap/css/bootstrap.min.css" />
    <link rel="stylesheet" href="/css/common.css" />
  </head>
  <body>
    <nav>
      <h1>Fairdesk</h1>
    </nav>
    <div class="contain">
      <!-- Navigation bar -->
      <div class="side-nav">
        <div title="Expand" class="nav-items nav-toggle">
          <i class="fa-solid fa-bars" style="color: #ffffff"></i>
          <p class="nav-items-para">Navigation</p>
        </div>        
        <div title="Client Data" class="nav-items" onclick="window.location.href = '/fairdesk/form/client'">
          <i class="fa-solid fa-user" style="color: #ffffff"></i>
          <p class="nav-items-para">Client Data</p>
        </div>

        <div class="nav-opt-wrap">
        <div title="Datasheet" class="datasheet-toggle nav-items">
          <i class="fa-solid fa-square-caret-down" style="color: #ffffff;"></i>
          <p class="nav-items-para">Datasheet</p>
        </div>

        <div class="nav-labels-opt">
          <div title="Labels" class="nav-items" onclick="window.location.href = '/fairdesk/form/labels'">
            <i class="fa-solid fa-address-card" style="color: #ffffff"></i>
            <p class="nav-items-para">Labels</p>
          </div>       
          <div title="TTR" class="nav-items" onclick="window.location.href = '/fairdesk/form/ttr'">
            <i class="fa-solid fa-ribbon" style="color: #ffffff"></i>
            <p class="nav-items-para">TTR</p>
          </div>
          <div title="Tape" class="nav-items" onclick="window.location.href = '/fairdesk/form/tape'">
            <i class="fa-solid fa-tape" style="color: #ffffff;"></i>
            <p class="nav-items-para">Tape</p>
          </div>  
        </div>          
        </div>
        
        <div class="nav-opt-wrap">
        <div title="Care" class="care-toggle nav-items">
          <i class="fa-solid fa-circle-chevron-down" style="color: #ffffff;"></i>
          <p class="nav-items-para">Care</p>
        </div>

        <div class="nav-care-opt">
        <div title="System ID" class="nav-items" onclick="window.location.href = '/fairdesk/form/systemid'">
          <i class="fa-solid fa-id-badge" style="color: #ffffff;"></i>
          <p class="nav-items-para">System ID</p>
        </div> 
        <div title="Care Call Report" class="nav-items" onclick="window.location.href = '/fairdesk/form/carecallreport'" >
          <i class="fa-solid fa-file" style="color: #ffffff"></i>
          <p class="nav-items-para">Call Report</p>
        </div>
        <div title="Care Workshop Report" class="nav-items active" onclick="window.location.href = '/fairdesk/form/careworkshopreport'">
          <i class="fa-solid fa-shop" style="color: #ffffff;"></i>
          <p class="nav-items-para">Workshop Report</p>
        </div>
        <div title="Carelead" class="nav-items" onclick="window.location.href = '/fairdesk/form/carelead'">
          <i class="fa-solid fa-notes-medical" style="color: #ffffff"></i>
          <p class="nav-items-para">Lead</p>
        </div>           
        <div title="Care Quote" class="nav-items" onclick="window.location.href = '/fairdesk/form/carequote'">
          <i class="fa-solid fa-quote-left" style="color: #ffffff;"></i>
          <p class="nav-items-para">Quote</p>
        </div>           
        </div>             
        </div>
        <div title="Rate Calculator" class="nav-items" onclick="window.location.href = '/fairdesk/form/ratecalculator'">
          <i class="fa-solid fa-calculator" style="color: #ffffff"></i>
          <p class="nav-items-para">Rate Calculator</p>
        </div>

        <div class="nav-opt-wrap">
        <div title="Rate Calculator" class="calc-toggle nav-items">
          <i class="fa-solid fa-square-caret-down" style="color: #ffffff;"></i>
          <p class="nav-items-para">Rate Calculator</p>
        </div>

        <div class="nav-calc-opt">
          <div title="Sales Calculator" class="nav-items" onclick="window.location.href = '/fairdesk/form/salescalc'">
            <i class="fa-solid fa-money-bill-transfer" style="color: #ffffff;"></i>
            <p class="nav-items-para">Sales Calculator</p>
          </div>       
          <div title="Production Calculator" class="nav-items" onclick="window.location.href = '/fairdesk/form/prodcalc'">
            <i class="fa-solid fa-industry" style="color: #ffffff;"></i>
            <p class="nav-items-para">Production Calculator</p>
          </div>
        </div>            
        </div>
      </div>

      <!-- The main content -->
      <div class="main-content">
        <form action="/fairdesk/form/datasheet" method="post" class="needs-validation" novalidate>
          <div class="span-full form-heading">
            <h1>Care Wokshop report</h1>
          </div>

          <div class="span-full form-subheading">
            <h1>Basic Identification</h1>
          </div>
          
          <div class="span-dual">
            <label for="wk-call-report-code">Call Report Code</label>
            <input type="text" id="wk-call-report-code" class="form-control" name="wkCallReportCode" placeholder="Enter Call Report Code" required />
          </div>          
          <div class="span-dual">
            <label for="wk-sys-id">System ID</label>
            <input type="text" id="wk-sys-id" class="form-control" name="wkSysId" placeholder="System ID" required />
          </div>          
          <div class="span-dual">
            <label for="wk-client-name">Client Name</label>
            <input type="text" id="wk-client-name" class="form-control" name="clientName" placeholder="Enter client name" required />
          </div>    
          <div class="span-dual">
            <label for="wk-inward-date">Inward date</label>
            <input type="date" id="wk-inward-date" class="form-control" name="InwardDate" placeholder="Enter inward date" required />
          </div>    
          <!-- <div class="span-dual">
            <label for="wk-outward-date">Outward date</label>
            <input type="date" id="wk-inward-report" class="form-control" name="clientName" placeholder="Enter outward date" required />
          </div> -->

          <div class="span-full form-subheading">
            <h1>Client & Contact Details</h1>
          </div>
          
          <div class="span-dual">
            <label for="wk-wk-client">Client</label>
            <input type="text" id="wk-wk-client" class="form-control" name="wkClient" placeholder="Enter client" required />
          </div>          
          <div class="span-dual">
            <label for="wk-user-name">User Name</label>
            <input type="text" id="wk-user-name" class="form-control" name="wkUserName" placeholder="Enter User Name" required />
          </div>          
          <div class="span-dual">
            <label for="wk-contact-no">Contact no</label>
            <input type="number" id="wk-contact-no" class="form-control" name="wkContactNo" placeholder="Enter outward date" required />
          </div>
          <div class="span-dual">
            <label for="wk-location">Location</label>
            <input type="text" id="wk-location" class="form-control" name="wkLocation" placeholder="Enter location" required />
          </div>          
          
          <div class="span-full form-subheading">
            <h1>Product Information</h1>
          </div>

          <div class="span-dual">
            <label for="wk-product">Product</label>
            <input type="text" id="wk-product" class="form-control" name="wkProduct" placeholder="Enter product" required />
          </div>          
          <div class="span-dual">
            <label for="wk-make">Make</label>
            <input type="text" id="wk-make" class="form-control" name="wkMake" placeholder="Enter make" required />
          </div>          
          <div class="span-dual">
            <label for="wk-model">Model</label>
            <input type="text" id="wk-model" class="form-control" name="wkModel" placeholder="Enter model" required />
          </div>          
          <div class="span-dual">
            <label for="wk-prod-serial-no">Serial no</label>
            <input type="text" id="wk-prod-serial-no" class="form-control" name="wkProdSerialNo" placeholder="Enter serial no" required />
          </div>
          
          <div class="span-full form-subheading">
            <h1>Issue & Diagnostics</h1>
          </div>
          <div class="span-dual">
            <label for="wk-symptoms">Serial no</label>
            <input type="text" id="wk-symptoms" class="form-control" name="wkSymptoms" placeholder="Enter symptoms" required />
          </div>
          <div class="span-dual">
            <label for="wk-observations">Observations</label>
            <input type="text" id="wk-observations" class="form-control" name="wkObservations" placeholder="Enter observations" required />
          </div>
          <div class="span-dual">
            <label for="wk-info-to-client">Info to clients</label>
            <input type="text" id="wk-info-to-client" class="form-control" name="wkInfoToClient" placeholder="Enter info to client" required />
          </div>
          <div class="span-dual">
            <label for="wk-requirments">Requirments</label>
            <input type="text" id="wk-requirments" class="form-control" name="wkRequirments" placeholder="Enter requirments" required />
          </div>

          <div class="span-full form-subheading">
            <h1>Service & Action</h1>
          </div>
          
          <div class="span-dual">
            <label for="wk-action-taken">Action taken</label>
            <input type="text" id="wk-action-taken" class="form-control" name="wkActionTaken" placeholder="Enter action taken" required />
          </div>          
          <div class="span-dual">
            <label for="wk-model">Mode</label>
            <input type="text" id="wk-mode" class="form-control" name="wkMode" placeholder="Enter Mode" required />
          </div>
          <div class="span-dual">
            <label for="wk-att-closedby">Attd & Closed by</label>
            <input type="text" id="wk-att-closedby" class="form-control" name="wkAttdClosedby" placeholder="Enter Attd & Closed by" required />
          </div>          
          <div class="span-dual">
            <label for="wk-amount">Amount</label>
            <input type="text" id="wk-amount" class="form-control" name="wkAmount" placeholder="Enter amount" required />
          </div>
          
          <div class="span-full form-subheading">
            <h1>Quotation & Sales</h1>
          </div>
          <div class="span-half">
            <label for="wk-remark">Remark</label>
            <input type="number" id="wk-remark" class="form-control" name="wkRemark" placeholder="Enter remark" required />
          </div>                
          <div class="span-half">
            <label for="wk-remark-1">Remark 1</label>
            <input type="number" id="wk-remark-1" class="form-control" name="wkRemarkOne" placeholder="Enter remark 1" required />
          </div>

          <div class="btn-grp span-full">
            <button type="submit">Submit Care Workshop Report</button>
          </div>               
        </form> 
      </div>
    </div>

    <script src="/js/common.js"></script>
  </body>
</html>
